{
	"cSpell.words": [
		"CMAKE",
		"CROSSCOMPILING",
		"Ctags",
		"Doxyfile",
		"Rockchip",
		"STRUCT",
		"aarch",
		"autogen",
		"doxygen",
		"doxywizard"
	],
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,

		//Ignore Qt Creator project files
		"{src,test}/*.files": true,
		"{src,test}/*.creator.user": true,
		"{src,test}/*.creator": true,
		"{src,test}/*.includes": true,
		"{src,test}/*.config": true

	},
	"files.associations": {
		"system_error": "cpp",
		"*.ipp": "cpp"
	}
}
